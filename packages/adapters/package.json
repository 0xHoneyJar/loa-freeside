      "import": "./dist/wizard/index.js",
      "types": "./dist/wizard/index.d.ts"
    },
    "./synthesis": {
      "import": "./dist/synthesis/index.js",
      "types": "./dist/synthesis/index.d.ts"
    },
    "./security": {
      "import": "./dist/security/index.js",
      "types": "./dist/security/index.d.ts"
    },
    "./coexistence": {
      "import": "./dist/coexistence/index.js",
      "types": "./dist/coexistence/index.d.ts"
    },
    "./agent": {
      "import": "./dist/agent/index.js",
      "types": "./dist/agent/index.d.ts"
    },
    "./agent/config": {
      "import": "./dist/agent/config.js",
      "types": "./dist/agent/config.d.ts"
    },
    "./agent/agent-auth-middleware": {
      "import": "./dist/agent/agent-auth-middleware.js",
      "types": "./dist/agent/agent-auth-middleware.d.ts"
    },
    "./agent/budget-manager": {
      "import": "./dist/agent/budget-manager.js",
