    "./agent/byok-manager": {
      "import": "./dist/agent/byok-manager.js",
      "types": "./dist/agent/byok-manager.d.ts"
    },
    "./agent/s2s-auth-middleware": {
      "import": "./dist/agent/s2s-auth-middleware.js",
      "types": "./dist/agent/s2s-auth-middleware.d.ts"
    },
    "./agent/s2s-jwt-validator": {
      "import": "./dist/agent/s2s-jwt-validator.js",
      "types": "./dist/agent/s2s-jwt-validator.d.ts"
    },
    "./agent/usage-receiver": {
      "import": "./dist/agent/usage-receiver.js",
      "types": "./dist/agent/usage-receiver.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@0xhoneyjar/loa-hounfour": "github:0xHoneyJar/loa-hounfour#b6e0027aea2f6e9c26726e29b44edbc1f3e3e290",
    "@arrakis/core": "file:../core",
    "drizzle-orm": "^0.33.0",
    "ioredis": "^5.9.2",
    "jose": "^5.10.0",
    "opossum": "^8.1.0",
    "postgres": "^3.4.0",
    "uuid": "^11.1.0",
    "viem": "^2.21.0",
    "aws-embedded-metrics": "^4.2.0",
    "@aws-sdk/client-secrets-manager": "^3.700.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "bullmq": "^5.66.0",
    "@types/node": "^20.0.0",
    "@types/opossum": "^8.1.7",
    "@types/uuid": "^10.0.0",
