{
  "name": "@arrakis/cli",
  "version": "0.1.0",
  "description": "Arrakis Developer CLI - gaib command (Voice from the Outer World)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gaib": "./dist/bin/gaib.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "dev": "tsx src/bin/gaib.ts"
  },
  "dependencies": {
    "@arrakis/sandbox": "file:../sandbox",
    "@aws-sdk/client-dynamodb": "^3.700.0",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/credential-providers": "^3.700.0",
    "@discordjs/rest": "^2.4.0",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "discord-api-types": "^0.37.100",
    "js-yaml": "^4.1.0",
    "ora": "^8.0.1",
    "ms": "^2.1.3",
    "postgres": "^3.4.0",
    "yaml": "^2.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/ms": "^2.1.0",
    "@types/node": "^20.0.0",
    "pino": "^9.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "pino": ">=8.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "UNLICENSED"
}
