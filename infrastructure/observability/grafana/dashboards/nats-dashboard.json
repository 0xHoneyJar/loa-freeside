      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "id": 21,
      "title": "Message Processing Latency (p99)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(nats_consumer_message_processing_duration_seconds_bucket[5m])) by (le, consumer))",
          "legendFormat": "{{ consumer }}"
        }
      ]
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
