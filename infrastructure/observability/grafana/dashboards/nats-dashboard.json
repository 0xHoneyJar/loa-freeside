      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(nats_consumer_messages_processed_total{status!=\"success\"}[1m])) by (consumer, status) * 60",
          "legendFormat": "{{ consumer }} ({{ status }})"
        }
      ]
    },
